// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

 // console.log(userInput);
var N=userInput;
if(N%13===0)
{
    console.log("yes");
    
}
else
{
    console.log("no");
}
  //end-here
});