// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

 // console.log(userInput);
 var data=userInput[0].split(" ");
A=parseFloat(data[0]);
B=parseFloat(data[1]);
C=parseFloat(data[2]);
if(A,B,C <= 100000)
{
    if((A*A) + (B*B) === (C*C))
    {
        console.log("yes");
    }
    else
    {
        console.log("no")
    }
}
  //end-here
});