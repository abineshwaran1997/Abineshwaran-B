// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

  var n=userInput[0];
var index='';
for(var i=0;i<=n.length;i++)
{
     index=index+n[i]+" ";
}
 console.log(n[0]+n[2])

  //end-here
});