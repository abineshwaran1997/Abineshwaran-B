// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 
var S =userInput[0];
//console.log(userInput[0])
function pal(S)
{
  var i=0; 
var j=S.length-1;
while(i<j)
{
    if(S[i]==S[j])
    {
        i++;
        j--;
    }
    else
    {
        return 'no';
    }
}
return 'yes';
}
console.log(pal(S));
  //console.log(userInput);

  //end-here
});